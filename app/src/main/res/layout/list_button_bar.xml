<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:fbutton="http://schemas.android.com/apk/res-auto"
    style="@style/commonListItemStyle"
    android:layout_width="match_parent"
    android:layout_height="48dp"
    android:background="@drawable/bg_swipe_item_neutral">

    <FrameLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/bg_item_normal_state"
        android:clickable="true"
        android:foreground="?attr/selectableItemBackground"
        tools:ignore="UselessParent"
        android:orientation="horizontal"
        android:gravity="center_horizontal">


        <LinearLayout
            android:id="@+id/LinearLayout02"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:gravity="center">
            <com.fammeo.app.font.FButton
                android:id="@+id/profile_button"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:text="@string/fa_heart"
                android:drawablePadding="2dp"
                android:textColor="@android:color/white"
                fbutton:FButton_FAIconColor="@android:color/white"
                fbutton:cornerRadius="10dp"
                fbutton:shadowEnabled="true"
                fbutton:shadowHeight="3dp"
                fbutton:FButton_ButtonText=""
                fbutton:FButton_FAIconMarginTop="-1dp"
                fbutton:FButton_FAIconText="@string/fa_eye"
                fbutton:FButton_FAIconSize="30"
                android:gravity="center"/>
            <com.fammeo.app.font.FButton
                android:id="@+id/call_button"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:text="@string/fa_heart"
                android:layout_marginLeft="10dp"
                android:drawablePadding="2dp"
                android:textColor="@android:color/white"
                fbutton:FButton_FAIconColor="@android:color/white"
                fbutton:cornerRadius="10dp"
                fbutton:shadowEnabled="true"
                fbutton:shadowHeight="3dp"
                fbutton:FButton_ButtonText=""
                fbutton:FButton_FAIconMarginTop="0dp"
                fbutton:FButton_FAIconText="@string/fa_call"
                fbutton:FButton_FAIconSize="30"
                fbutton:buttonColor="@color/fbutton_color_belize_hole"
                android:gravity="center"/>

            <com.fammeo.app.font.FButton
                android:id="@+id/email_button"
                android:layout_width="50dp"
                android:layout_marginLeft="10dp"
                fbutton:FButton_FAIconColor="@android:color/white"
                android:layout_height="wrap_content"
                android:text="@string/fa_heart"
                android:drawablePadding="2dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:paddingTop="-5dp"
                android:textColor="@android:color/white"
                fbutton:cornerRadius="10dp"
                fbutton:shadowEnabled="true"
                fbutton:shadowHeight="3dp"
                fbutton:FButton_ButtonText=""
                fbutton:FButton_FAIconMarginTop="0dp"
                fbutton:FButton_FAIconText="@string/fa_envelope_o"
                fbutton:FButton_FAIconSize="30"
                fbutton:buttonColor="@color/fbutton_color_green_sea"
                android:gravity="center"/>
            <com.fammeo.app.font.FButton
                android:id="@+id/delete_button"
                android:layout_width="50dp"
                android:layout_marginLeft="10dp"
                fbutton:FButton_FAIconColor="@android:color/white"
                android:layout_height="wrap_content"
                android:text="@string/fa_heart"
                android:drawablePadding="2dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:paddingTop="-5dp"
                android:textColor="@android:color/white"
                fbutton:cornerRadius="10dp"
                fbutton:shadowEnabled="true"
                fbutton:shadowHeight="3dp"
                fbutton:FButton_ButtonText=""
                fbutton:FButton_FAIconMarginTop="0dp"
                fbutton:FButton_FAIconText="@string/fa_trash"
                fbutton:FButton_FAIconSize="30"
                fbutton:buttonColor="@color/theme_danger"
                android:gravity="center"/>

        </LinearLayout>




    </FrameLayout>




</FrameLayout>